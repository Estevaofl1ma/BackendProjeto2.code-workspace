generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Transaction {
  id     Int    @id @default(autoincrement())
  titulo String
  valor  Int
}

model Category {
  id   Int    @id @default(autoincrement())
  nome String
}

model Bank {
  id   Int    @id @default(autoincrement())
  nome String
}
